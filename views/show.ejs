<%- include("partials/header") -%>

<div class="container">
    <div class="row">
        <div class="col align-self-center">
            <div class="card">
                <img class="img-responsive" src="<%= project.image %>" style="height: 600px;">
                <div class="caption-full caption">
                    <h4><%= project.name %></h4>
                    <p><%= project.description %></p>
                    <a href="<%= project.githubLink %>">Check out the code</a>
                </div>
            </div>
        </div>
    </div>
</div>

<%- include("partials/footer") -%>